{
  "name": "n8n-expert-consultant",
  "version": "1.1.0",
  "description": "Expert consultant IA spécialisé en n8n pour Claude Code - Utilise n8n-mcp et n8n-skills officiels",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run create-dirs",
    "create-dirs": "mkdir -p workflows templates documentation logs",
    "mcp:start": "npx n8n-mcp",
    "mcp:test": "npx n8n-mcp --help",
    "mcp:telemetry-disable": "npx n8n-mcp telemetry disable",
    "start": "echo 'Lancez Claude Code dans ce dossier pour utiliser le consultant n8n'",
    "test": "echo \"No tests configured yet\"",
    "skills:list": "ls -la skills/skills/",
    "skills:update": "cd skills && git pull origin main"
  },
  "keywords": [
    "n8n",
    "automation",
    "workflows",
    "consultant",
    "ai",
    "claude",
    "mcp",
    "n8n-mcp",
    "n8n-skills"
  ],
  "author": "N8N Expert Consultant Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czlonkowski/n8n-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/czlonkowski/n8n-mcp/issues"
  },
  "homepage": "https://github.com/czlonkowski/n8n-mcp#readme",
  "devDependencies": {
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "mcp": {
    "servers": {
      "n8n-mcp": "n8n-mcp"
    },
    "skills": {
      "source": "./skills/skills/",
      "installed": [
        "n8n-expression-syntax",
        "n8n-mcp-tools-expert",
        "n8n-workflow-patterns",
        "n8n-validation-expert",
        "n8n-node-configuration",
        "n8n-code-javascript",
        "n8n-code-python"
      ]
    }
  },
  "type": "commonjs",
  "dependencies": {
    "n8n-mcp": "^2.33.5"
  }
}
